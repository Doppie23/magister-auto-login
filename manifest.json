{
    "name": "Magister auto login",
    "description": "auto login magister.",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
    "storage"
    ],
    "action":{
        "default_title": "Magister auto login",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
         "matches": ["http://accounts.magister.net/*","https://accounts.magister.net/*"],
         "js": ["script.js"]
        }
       ]
    
  }